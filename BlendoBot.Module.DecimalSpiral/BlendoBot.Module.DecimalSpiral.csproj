<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Authors>Biendeo</Authors>
    <Company>BlendoBot</Company>
    <PackageId>BlendoBot.Module.DecimalSpiral</PackageId>
    <Version>1.0.0</Version>
    <RepositoryUrl>https://github.com/BlendoBot/BlendoBot.Module.DecimalSpiral</RepositoryUrl>
    <PackageProjectUrl>https://github.com/BlendoBot/BlendoBot.Module.DecimalSpiral</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>discord;bot;module</PackageTags>
    <Description>Makes a pretty spiral.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BlendoBot.Core" Version="1.1.0" />
    <PackageReference Include="MinVer" Version="2.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
